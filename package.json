{
  "name": "react-input-children",
  "version": "2.0.0",
  "description": "A replacement for the base <input> component capable of rendering a child (link, button...) inside the input itself",
  "main": "lib",
  "files": [
    "lib",
    "src",
    "examples",
    "styleguide"
  ],
  "scripts": {
    "test": "jest",
    "build": "rm -rf lib && mkdir lib && tsc",
    "preversion": "npm run test",
    "prepublish": "npm run build",
    "start": "styleguidist server",
    "release-version": "smooth-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/buildo/react-input-children.git"
  },
  "keywords": [
    "react",
    "react-component",
    "input",
    "children",
    "form",
    "label",
    "action",
    "button"
  ],
  "author": "Francesco Cioria <francesco@buildo.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/buildo/react-input-children/issues"
  },
  "homepage": "https://github.com/buildo/react-input-children",
  "devDependencies": {
    "@types/enzyme": "2.8.6",
    "@types/jest": "20.0.7",
    "babel-loader": "^7.1.2",
    "babel-preset-buildo": "^0.1.1",
    "css-loader": "^0.28.5",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "file-loader": "^1.1.5",
    "jest": "^21.2.1",
    "node-sass": "4.5.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "raw-loader": "^0.5.1",
    "react": "^16",
    "react-docgen-typescript": "^1.1.0",
    "react-dom": "^16",
    "react-styleguidist": "^6.0.33",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "smooth-release": "^8.0.4",
    "ts-jest": "^21.2.3",
    "ts-loader": "^2.3.3",
    "typescript": "^2.4.2",
    "webpack": "3.5.5"
  },
  "peerDependencies": {
    "react": ">= 15"
  },
  "dependencies": {
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.25",
    "prop-types": "^15.6.0"
  },
  "jest": {
    "setupFiles": ["<rootDir>/test/setup.js"],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(.*[.](test))[.](tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
